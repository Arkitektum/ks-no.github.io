      "post" : {
        "tags" : [ "DokumentlagerUpload" ],
        "summary" : "Laster opp kombinert metadata og fildata for et dokument",
        "description" : "",
        "operationId" : "uploadMetadataAndDokument",
        "consumes" : [ "multipart/form-data" ],
        "parameters" : [ {
          "name" : "fiksOrganisasjonId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "kontoId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "dokumentfilnavn",
          "in" : "formData",
          "required" : true,
          "type" : "string"
        }, {
          "name" : "mimetype",
          "in" : "formData",
          "required" : true,
          "type" : "string"
        }, {
          "name" : "ttl",
          "in" : "formData",
          "required" : true,
          "type" : "integer",
          "format" : "int64"
        }, {
          "name" : "eksponertFor",
          "in" : "formData",
          "required" : true,
          "type" : "array",
          "items" : {
            "type" : "string"
          },
          "collectionFormat" : "multi"
        }, {
          "name" : "dokument",
          "in" : "formData",
          "required" : true,
          "type" : "file"
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/FilMetadataUploadResult"
            }
          }
        }
      }
    },
    "/dokumentlager/api/v1/{fiksOrganisasjonId}/kontoer/{kontoId}/dokumenter/metadata" : {
      "post" : {
        "tags" : [ "DokumentlagerUpload" ],
        "summary" : "Laster opp metadata for et dokument",
        "description" : "",
        "operationId" : "uploadMetadata",
        "parameters" : [ {
          "name" : "fiksOrganisasjonId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "kontoId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "in" : "body",
          "name" : "body",
          "required" : false,
          "schema" : {
            "$ref" : "#/definitions/FilMetadataUpload"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/FilMetadataUploadResult"
            }
          }
        }
      }
    },
    "/dokumentlager/api/v1/{fiksOrganisasjonId}/kontoer/{kontoId}/dokumenter/{dokumentId}" : {
      "delete" : {
        "tags" : [ "DokumentlagerUpload" ],
        "summary" : "Sletter dokumentet med gitt id",
        "description" : "",
        "operationId" : "deleteDokument",
        "parameters" : [ {
          "name" : "fiksOrganisasjonId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "kontoId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "dokumentId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "type" : "object"
            }
          }
        }
      }
    },
    "/dokumentlager/api/v1/{fiksOrganisasjonId}/kontoer/{kontoId}/dokumenter/{dokumentId}/data" : {
      "post" : {
        "tags" : [ "DokumentlagerUpload" ],
        "summary" : "Laster opp et dokument for dokumentid",
        "description" : "",
        "operationId" : "uploadDokument",
        "consumes" : [ "multipart/form-data" ],
        "parameters" : [ {
          "name" : "fiksOrganisasjonId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "kontoId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "dokumentId",
          "in" : "path",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "dokument",
          "in" : "formData",
          "required" : true,
          "type" : "file"
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "type" : "object"
            }
          }
        }
      }
    }
  },
  "definitions" : {
    "FilMetadataUpload" : {
      "type" : "object",
      "required" : [ "dokumentfilnavn", "eksponertFor", "mimetype", "ttl" ],
      "properties" : {
        "dokumentfilnavn" : {
          "type" : "string",
          "description" : "Dokumentets filnavn. Blir navnet på filen ved nedlasting.",
          "minLength" : 0,
          "maxLength" : 226
        },
        "mimetype" : {
          "type" : "string",
          "description" : "Dokumentets MIME-type. Blir mimetype til filen når den lastes ned av mottaker"
        },
        "ttl" : {
          "type" : "integer",
          "format" : "int64",
          "description" : "Tidspunkt for når filen blir utilgjengeliggjort automatisk. Oppgis i millisekunder UNIX epoch. En negativ verdi betyr at dokumentet aldri utløper."
        },
        "eksponertFor" : {
          "type" : "array",
          "description" : "En liste av personer og/eller organisasjoner som har lov til å laste ned og lese dokumentet",
          "uniqueItems" : true,
          "items" : {
            "type" : "string"
          }
        }
      },
      "description" : "Metadata for et dokument"
    },
    "FilMetadataUploadResult" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "string",
          "format" : "uuid",
          "description" : "Id gitt til opplastet metadata, som brukes for å laste opp dokumentet"
        }
      },
      "description" : "Informasjon om opplastet metadata for et dokument"
    }
  }
}