{
  "swagger" : "2.0",
  "info" : {
    "version" : "v1",
    "title" : "KS FIKS: Dokumentlager Upload Service",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "http://www.ks.no/fagomrader/utvikling/digitalisering/digitaliseringsstrategien/fiks/",
      "email" : "fiks@ks.no"
    }
  },
  "basePath" : "/api",
  "tags" : [ {
    "name" : "DokumentlagerUpload"
  } ],
  "schemes" : [ "https" ],
  "paths" : {
