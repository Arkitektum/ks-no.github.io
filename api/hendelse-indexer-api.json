{
  "swagger" : "2.0",
  "info" : {
    "version" : "v1",
    "title" : "KS Fiks: Hendelse Indexer Service",
    "contact" : {
      "name" : "Fiks Utvikling",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    }
  },
  "basePath" : "/api",
  "tags" : [ {
    "name" : "Index"
  } ],
  "schemes" : [ "http", "https" ],
  "paths" : {
    "/hendelseindexer/api/deleteRequest" : {
      "put" : {
        "tags" : [ "Index" ],
        "summary" : "Remove events for the specified fiks-org from the index",
        "description" : "",
        "operationId" : "deleteHendelsesgrupper",
        "parameters" : [ {
          "name" : "integrasjonId",
          "in" : "query",
          "description" : "Id for integrasjonen, som registret i fiks-konfigurasjon",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "fiksOrganisasjonId",
          "in" : "query",
          "description" : "Fiks-organisasjon id",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "name" : "enhetId",
          "in" : "query",
          "description" : "Enhet id",
          "required" : false,
          "type" : "string"
        }, {
          "name" : "gruppeId",
          "in" : "query",
          "description" : "Gruppe id",
          "required" : false,
          "type" : "string"
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/DeletionResponse"
            }
          }
        }
      }
    },
    "/hendelseindexer/api/indexRequest" : {
      "put" : {
        "tags" : [ "Index" ],
        "summary" : "Add new groups to the index",
        "description" : "Submit one or more groups to be indexed, up to a maximum of 5000. Note that this operation is not atomic: the response will indicate any groups where indexing failed.",
        "operationId" : "indexHendelsegruppe",
        "parameters" : [ {
          "name" : "integrasjonId",
          "in" : "query",
          "description" : "Id for integrasjonen, som registret i fiks-konfigurasjon",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        }, {
          "in" : "body",
          "name" : "body",
          "description" : "The groups to be indexed",
          "required" : false,
          "schema" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/definitions/IndexRequest"
            }
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "The bulk request has been completed. Note that individual elements may still have failed, as indicated by the \"hasFailures\" property",
            "schema" : {
              "$ref" : "#/definitions/BulkResponse"
            }
          },
          "400" : {
            "description" : "The request is malformed and will not be processed",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Field validation error"
              }
            }
          },
          "422" : {
            "description" : "The bulk request is properly formatted but do not conform to limitations such as max number of elements (5000)"
          },
          "500" : {
            "description" : "The bulk request has has failed. An unknown number of items may still have been indexed, but all should be resubmitted to ensure completion"
          }
        }
      }
    }
  },
  "definitions" : {
    "BulkResponse" : {
      "type" : "object",
      "properties" : {
        "results" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/IndexResponse"
          }
        },
        "hasFailures" : {
          "type" : "boolean"
        },
        "failed" : {
          "type" : "boolean"
        },
        "error" : {
          "type" : "string"
        }
      },
      "description" : "Result of a bulk request"
    },
    "DeletionResponse" : {
      "type" : "object",
      "properties" : {
        "deletedGroups" : {
          "type" : "integer",
          "format" : "int64"
        },
        "failureId" : {
          "type" : "string"
        },
        "operationFailed" : {
          "type" : "boolean"
        },
        "deletionsFailed" : {
          "type" : "boolean"
        },
        "failedDeletionsGroupId" : {
          "type" : "array",
          "uniqueItems" : true,
          "items" : {
            "type" : "string"
          }
        }
      }
    },
    "Dokument" : {
      "type" : "object",
      "required" : [ "id" ],
      "properties" : {
        "id" : {
          "type" : "string"
        },
        "tittel" : {
          "type" : "string"
        },
        "size" : {
          "type" : "integer",
          "format" : "int32"
        },
        "nedlastingLenke" : {
          "type" : "string"
        }
      },
      "description" : "Et dokument for en forsendelse"
    },
    "Enhet" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "string"
        },
        "visningsnavn" : {
          "type" : "string"
        }
      }
    },
    "Field validation error" : {
      "type" : "object",
      "properties" : {
        "elementId" : {
          "type" : "string",
          "description" : "The id of the failed element, if available"
        },
        "field" : {
          "type" : "string",
          "description" : "The field path"
        },
        "message" : {
          "type" : "string",
          "description" : "The validation error message"
        }
      }
    },
    "FiksOrganisasjon" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "string",
          "format" : "uuid"
        },
        "visningsnavn" : {
          "type" : "string"
        }
      }
    },
    "Forsendelse" : {
      "allOf" : [ {
        "$ref" : "#/definitions/TypeMetadata"
      }, {
        "type" : "object",
        "properties" : {
          "tittel" : {
            "type" : "string"
          },
          "avsender" : {
            "type" : "string",
            "description" : "Navn på person eller orgnr / navn på organisasjon som er kilde for forsendelsen."
          },
          "mottaker" : {
            "type" : "string",
            "description" : "Navn på person eller orgnr / navn på organisasjon som er mottaker for forsendelsen"
          },
          "lest" : {
            "type" : "boolean",
            "description" : "Indikerer om forsendelsen er lest eller ikke"
          },
          "svarLenke" : {
            "type" : "string",
            "description" : "Url som skal benyttes for å svare på denne forsendelsen"
          },
          "dokumenter" : {
            "type" : "array",
            "description" : "Dokumenter som inngår i denne forsendelsen",
            "items" : {
              "$ref" : "#/definitions/Dokument"
            }
          }
        },
        "description" : "Hendelse som beskriver en forsendelse"
      } ]
    },
    "IndexRequest" : {
      "type" : "object",
      "required" : [ "dato", "eksponertFor", "fiksOrganisasjon", "id", "innloggingsniva", "typeMetadata" ],
      "properties" : {
        "id" : {
          "type" : "string"
        },
        "innloggingsniva" : {
          "type" : "integer",
          "format" : "int32",
          "minimum" : 0
        },
        "eksponertFor" : {
          "type" : "string"
        },
        "fiksOrganisasjon" : {
          "$ref" : "#/definitions/FiksOrganisasjon"
        },
        "korrelasjonId" : {
          "type" : "string"
        },
        "parent" : {
          "$ref" : "#/definitions/Parent"
        },
        "enhet" : {
          "$ref" : "#/definitions/Enhet"
        },
        "dato" : {
          "type" : "integer",
          "format" : "int64"
        },
        "typeMetadata" : {
          "$ref" : "#/definitions/TypeMetadata"
        }
      },
      "description" : "En forespørsel om å opprette en ny gruppe, eller å oppdatere en eksisterende gruppe"
    },
    "IndexResponse" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "string"
        },
        "failed" : {
          "type" : "boolean"
        },
        "failureMessage" : {
          "type" : "string"
        }
      }
    },
    "Parent" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "string"
        },
        "type" : {
          "type" : "string",
          "enum" : [ "Forsendelse" ]
        }
      }
    },
    "TypeMetadata" : {
      "type" : "object",
      "required" : [ "type" ],
      "discriminator" : "type",
      "properties" : {
        "type" : {
          "type" : "string",
          "enum" : [ "Forsendelse" ]
        }
      }
    }
  }
}